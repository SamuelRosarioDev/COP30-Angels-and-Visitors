		"@biomejs/biome": "^1.9.4",
		"@types/bun": "latest",
		"@types/cors": "^2.8.18",
		"@types/express": "^5.0.1",
		"prisma": "^6.7.0",
		"tsc": "^2.0.4",
		"tsx": "^4.19.4",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"@prisma/client": "^6.7.0",
		"cors": "^2.8.5",
